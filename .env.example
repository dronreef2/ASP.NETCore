# Configuração de exemplo para o Tutor Copiloto ASP.NET Core
# Copie este arquivo para .env e configure com seus valores reais

# === Claude AI Configuration ===
AI__Claude__ApiKey=sk-ant-api03-YOUR_CLAUDE_API_KEY_HERE

# === Ngrok Configuration ===
Ngrok__AuthToken=YOUR_NGROK_AUTH_TOKEN_HERE

# === JWT Configuration ===
JWT__SecretKey=YOUR_JWT_SECRET_KEY_FOR_PRODUCTION_HERE

# === GitHub Webhook ===
GitHub__WebhookSecret=YOUR_GITHUB_WEBHOOK_SECRET_HERE

# === Database ===
ConnectionStrings__DefaultConnection=Data Source=tutorcopiloto.db
ConnectionStrings__Redis=localhost:6379

# === Deployment ===
Deployment__BaseUrl=https://your-deployment-url.com

# === Features ===
Features__EnableSwagger=true
Features__EnableDetailedErrors=true
Features__EnableSensitiveDataLogging=false

# === CORS Origins ===
CORS__Origins=http://localhost:3000,http://localhost:5173,vscode-webview://*

# === Legacy environment variables for compatibility ===
ANTHROPIC_API_KEY=sk-ant-api03-your-key-here
OPENAI_API_KEY=sk-your-openai-key-here  # Opcional para futuras integrações
LLAMAINDEX_API_KEY=llx-your-llamaindex-key-here  # Para RAG e embeddings
POSTGRES_URL=postgresql://tutor:copiloto123@localhost:5432/tutordb
REDIS_URL=redis://localhost:6379
PORT=8080
HOST=0.0.0.0
NODE_ENV=development
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
LOG_LEVEL=debug
ENABLE_METRICS=true
DEFAULT_MODEL=claude-3-5-sonnet-20241022
FALLBACK_MODEL=claude-3-haiku-20240307
MAX_TOKENS=4096
TEMPERATURE=0.7
DEFAULT_USER_LEVEL=intermediate
DEFAULT_PERSONA=tutor
DEFAULT_COST_MODE=deep-analysis
ENABLE_ANTI_CHEAT=true
SESSION_TIMEOUT_MINUTES=60
RATE_LIMIT_PER_MINUTE=100
ENABLE_RAG=true
ENABLE_TOOL_EXECUTION=true
ENABLE_CODE_ANALYSIS=true
ENABLE_TEST_GENERATION=true
ENABLE_ASSESSMENT=true
CACHE_TTL_SECONDS=3600
MAX_CONCURRENT_TOOLS=5
TOOL_TIMEOUT_SECONDS=120
VSCODE_DEV_MODE=true
VSCODE_API_URL=http://localhost:8080
REACT_APP_API_URL=http://localhost:8080
REACT_APP_WS_URL=ws://localhost:8080
